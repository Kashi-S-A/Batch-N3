<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="shopbean" class="com.ty.map.Shop">
		<property name="name" value="Gajanan Vada Pav Centre"></property>
		<property name="items">
			<map>
				<entry key="Vad Pav" value="15"></entry>
				<entry key="Samosa Pav" value="18"></entry>
				<entry key="Kachori" value="25"></entry>
				<entry key="Bhaji" value="20"></entry>
				<entry key="Jalebi" value="35"></entry>
			</map>
		</property>
	</bean>

</beans>